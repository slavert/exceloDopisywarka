<Window x:Class="ExceloDopisywarka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:ExceloDopisywarka"
        Title="ExceloDopisywarka" Height="350" Width="749" ResizeMode="CanMinimize">
    <Grid Margin="0,0,-6,0">
        <Button Content="Plik excel" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.649,-0.396" Click="Button_Click"/>
        <TextBlock x:Name="nazwaPlikuExcelForm" HorizontalAlignment="Left" Margin="123,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="0.45,-1.406" Width="386" Text="Wybierz plik do doczytania"/>
        <ComboBox x:Name="listaZakładekComboBox" HorizontalAlignment="Left" Margin="123,34,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="wczytajPlikButton" Content="Wczytaj plik" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="108" RenderTransformOrigin="0.649,-0.396" Click="Button_Click"/>
        <Button x:Name="wczytaZakładkęButton" Content="Wczytaj zakładkę" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" Width="108" Click="wczytaZakładkęButton_Click"/>
    </Grid>
</Window>